{
  "manifest_version": 3,
  "name": "Focus",
  "description": "Focus extension",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "Content.js",
        "dateTime.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "hello_extensions.png"
  }
}
